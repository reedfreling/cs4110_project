intro a; intro b; intro c;
create k;
k add worlds { w1, w2 };
k add accessibility w1 w2;
k add valuation w1 a;
k add valuation w1 c;
k add valuation w2 b;
intro v1; intro v2; intro v3; intro v4; intro v5;
v1 := k w1 ||- a;
v2 := k w1 ||- (a and b);
v3 := k w1 ||- (a and c);
v4 := k w2 ||- b;
v5 := k w2 ||- (a -> b);
print v1;
print v2;
print v3;
print v4;
print v5;
v5 := k w2 ||- (b -> a);
print v5;
intro v6;
v6 := k w1 ||- []a;
print v6;
v6 := k w1 ||- []b;
print v6