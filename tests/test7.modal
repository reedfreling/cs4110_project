intro p;
intro q;
create m;
m add worlds { w1, w2, w3 };
m add accessibility w1 w1;
m add accessibility w1 w2;
m add accessibility w2 w2;
m add accessibility w2 w3;
m add accessibility w3 w2;
m add valuation w2 p;
m add valuation w3 q;
intros {v1, v2, v3, v4, v5, v6, v7, v8, v9};
v1 := m w1 ||- <><>q;
v2 := m w1 ||- <>[]q;
v3 := m w1 ||- <><>p;
v4 := m w1 ||- <>[]p;
v5 := m w1 ||- <><><><><><><><><>p;
v6 := m w1 ||- <><><><><><><><>[]p;
m add valuation w1 p;
m add valuation w3 p;
v7 := m w1 ||- <><>(p and q);
v8 := m w1 ||- p and []p;
v9 := m w1 ||- p and <>(p and []p);
print v1;
print v2;
print v3;
print v4;
print v5;
print v6;
print v7;
print v8;
print v9